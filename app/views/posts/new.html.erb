<%= form_for @post, html: { multipart: true } do |f| %>
	<%= f.label :picture, "Picture:"%><br />
	<%= f.file_field :picture %><br />

	<%= f.label :caption, "Caption:" %><br />
	<%= f.text_area :caption %><br />

	<%= f.label :	, "Hashtags:" %><br />
	<%= f.text_field :hashtag_title, data: { role: 'tagsinput' } %><br />
	<br />
	<%= f.submit %>

<%end%>